{
    "name": "lynxlab/ada-module-classagenda",
    "description": "This is the ada classagenda module.",
    "license": "GPL",
    "authors": [
        {
            "name": "Giorgio Consorti",
            "email": "g.consorti@lynxlab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lynxlab\\ADA\\Module\\Classagenda\\": [
                "include/",
                "include/form",
                "include/management"
            ]
        }
    },
    "require": {
        "oomphinc/composer-installers-extender": "^2.0",
        "npm-asset/fullcalendar": "~3.9.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "js/vendor/{$name}/": [
                "type:npm-asset",
                "type:bower-asset"
            ]
        }
    }
}
